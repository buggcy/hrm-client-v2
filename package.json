{
  "name": "developer-portal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "http-server ./build",
    "lint": "next lint && npm run type-check",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:watch": "playwright test --ui",
    "prepare": "husky",
    "plop": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-toast": "1.1.5",
    "@sentry/nextjs": "7.113.0",
    "@sentry/react": "7.114.0",
    "@tanstack/eslint-plugin-query": "5.32.1",
    "@tanstack/react-query": "5.32.1",
    "@tanstack/react-query-devtools": "5.32.1",
    "axios": "1.6.8",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "firebase": "10.11.1",
    "logrocket": "8.1.0",
    "logrocket-react": "6.0.3",
    "lucide-react": "0.376.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.3",
    "tailwind-merge": "2.3.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.23.5"
  },
  "devDependencies": {
    "@playwright/test": "1.43.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.5",
    "@types/eslint": "8.56.10",
    "@types/jest": "29.5.12",
    "@types/logrocket-react": "3.0.3",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.3.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-playwright": "1.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-unused-imports": "3.2.0",
    "http-server": "14.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.2",
    "msw": "2.3.0",
    "plop": "4.0.1",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.3",
    "tsx": "4.8.2",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
